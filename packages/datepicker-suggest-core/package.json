{
  "name": "@datepicker-suggest/core",
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint . --max-warnings 0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "tsup": "^8.1.0",
    "typescript": "latest",
    "@types/node": "^20.11.24"
  },
  "peerDependencies": {
    "date-fns": "^3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivnbogdan/datepicker-suggest.git"
  },
  "keywords": [
    "datepicker",
    "autocomplete"
  ],
  "author": "ivnbogdan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivnbogdan/datepicker-suggest/issues"
  },
  "homepage": "https://github.com/ivnbogdan/datepicker-suggest#readme",
  "description": "",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "package.json"
  ]
}
