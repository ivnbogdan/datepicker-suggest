{
  "name": "@datepicker-suggest/react",
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build:css": "tailwindcss -i ./src/index.css -o ./dist/index.css --minify",
    "build": "tsup src/index.ts --format cjs,esm --dts && npm run build:css",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css"
  },
  "dependencies": {
    "@datepicker-suggest/core": "workspace:*"
  },
  "peerDependencies": {
    "@headlessui/react": "^2.1.2",
    "date-fns": "^3.6.0",
    "react": "^18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "tsup": "^8.1.0",
    "typescript": "^5.3.3",
    "tailwindcss": "^3.4.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivnbogdan/datepicker-suggest.git"
  },
  "keywords": [
    "datepicker"
  ],
  "author": "ivnbogdan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivnbogdan/datepicker-suggest/issues"
  },
  "homepage": "https://github.com/ivnbogdan/datepicker-suggest#readme",
  "description": "",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "package.json"
  ]
}
